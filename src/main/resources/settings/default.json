{
	"general": {
		"groupByEntryPoint": true,
		"eventTypes": "Logged Error,Uncaught Exception,Logged Warning,Swallowed Exception,HTTP Error,Caught Exception",
		"pointsWanted": 24,
		"splitLimit": 3
	},
	
	"transactions": {
		"groups": [
			{
				"name": " Servlets",
				"values": [
					"/^.*Servlet.*$/"
				]
			},
			
			{
				"name": " Tasks",
				"values": [
					"/^.*Task.*$/"
				]
			},
			
			{
				"name": " Messages",
				"values": [
					"/^.*Message.*$/"
				]
			}
			
		]
	},
	
	"applications": {
		"groups": [
			{
				"name": "prod-app",
				"values": [
					"prod-app1",
					"prod-app2"
				]
			}
		]
	},
	
	"tiers": 
		[
			{
				"names": [
					"org.apache.catalina",
					"org.apache.jasper",
					"org.apache.coyote",
					"org.apache.tomcat"
				],
				"labels": ["MyTomcat"]
			}
		]
	,
	
	"regression": {
		"activeTimespan": 1440,
		"minBaselineTimespan": 20160,
		"baselineTimespanFactor": 4,
		"minVolumeThreshold": 50,
		"minErrorRateThreshold": 0.1,
		"regressionDelta": 0.5,
		"criticalRegressionDelta": 1,
		"applySeasonality": true,
		"criticalExceptionTypes": "NullPointerException|IndexOutOfBoundsException|InvalidCastException|AssertionError",
		
		"sortOrder": "Severe New,Severe Regression,New,Regression",
		"typeOrder": "Logged Error,Uncaught Exception,Logged Warning,HTTP Error,Timer,Swallowed Exception,Caught Exception"
	},
	
	"regressionReport": {
		"newEventScore": 1,
		"regressionScore": 1,
		"severeNewEventScore": 2,
		"criticalRegressionScore": 2,
		"scoreWeight": 2.5
	}
}
